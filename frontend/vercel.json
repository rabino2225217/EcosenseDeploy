{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "outputDirectory": "build/client",
  "framework": null,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/build/client/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/build/client/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}

